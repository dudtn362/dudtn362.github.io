<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="rootPane" prefHeight="556.0" prefWidth="797.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.vision.popol.sqlex.SqlControll">
   <center>
      <TableView fx:id="tableSqlResult" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="280.0" prefWidth="797.0" spacing="5.0" style="-fx-background-color: lightgreen;" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="sqlEditor" opacity="0.84" prefHeight="270.0" prefWidth="683.0" promptText="질의를 넣으세요" style="-fx-background-color: lightgreen;" />
            <Button fx:id="btnCommit" mnemonicParsing="false" onAction="#commit" prefHeight="271.0" prefWidth="91.0" text="질의수행" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </top>
   <bottom>
      <Label style="-fx-background-color: darkgrey;" text="효성비젼직업전문학교 인공지능반 2기 질의연습기제작 ver 1.0" textFill="#ee1111" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
